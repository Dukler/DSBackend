{
  "Standalones": [
    {
      "ID": "email",
      "LazyID": "TextField",
      "ExtProperties": {
        "label": "email",
        "type": "text",
        "autoComplete": "",
        "margin": "normal",
        "variant": "outlined"
      },
      "Styles": {
        "width": "100%"
      }
    },
    {
      "ID": "password",
      "LazyID": "TextField",
      "ExtProperties": {
        "label": "password",
        "type": "password",
        "autoComplete": "",
        "margin": "normal",
        "variant": "outlined"
      },
      "Styles": {
        "width": "100%"
      }
    },
    {
      "ID": "createUser",
      "LazyID": "TextButton",
      "ExtProperties": {
        "label": "Crear Usuario",
        "variant": "contained",
        "color": "primary"
      },
      "Actions": {
        "onClick": "fml"
      },
      "Styles": {},
      "Container": {
        "item": true
      }
    },
    {
      "ID": "login",
      "LazyID": "TextButton",
      "ExtProperties": {
        "label": "Iniciar Sesion",
        "variant": "contained",
        "color": "primary"
      },
      "Actions": {
        "onClick": "login"
      },
      "Styles": {},
      "Container": {
        "item": true
      }
    }
  ],
  "Containers": [
    {
      "ID": "loginPaper",
      "LazyID": "Paper",
      "Standalones": [
        "loginForm",
        "loginGridButtons"
      ],
      "ExtProperties": {}
    },
    {
      "ID": "loginForm",
      "LazyID": "form",
      "Standalones": [
        "email",
        "password"
      ]
    },
    {
      "ID": "loginGridButtons",
      "LazyID": "Grid",
      "Standalones": [
        "createUser",
        "login"
      ],
      "ExtProperties": {
        "container": true,
        "direction": "row",
        "justify": "space-between",
        "alignItems": "flex-start"
      },
      "RenderStandalones": {
        "All": {
          "standalones": [
            "createUser",
            "login"
          ]
        }
      }
    }
  ]
}